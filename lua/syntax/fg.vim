" Define syntax highlighting for numbers                                                                                                                                                                                                                                                                                                                                                                                                                   
syntax match tsNumber /\v\d+(\.\d+)?(e[+-]?\d+)?/ contains=tsNumberSeparator                                                                                                                                                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for number separators (e.g., underscores in 1_000_000)                                                                                                                                                                                                                                                                                                                                                                        
syntax match tsNumberSeparator /_\+/                                                                                                                                                                                                                                                                                                                                                                                                                       
" Strings                                                                                                                                                                                                                                                                                                                                                                                                                                                  
syntax match flowwingString /".*"/                                                                                                                                                                                                                                                                                                                                                                                                                         
" Define syntax highlighting for strings                                                                                                                                                                                                                                                                                                                                                                                                                   
syntax match tsString /'[^']*'/ contained                                                                                                                                                                                                                                                                                                                                                                                                                  
syntax match tsString /"[^"]*"/ contained                                                                                                                                                                                                                                                                                                                                                                                                                  
syntax match tsString /`[^`]*`/ contained                                                                                                                                                                                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for comments                                                                                                                                                                                                                                                                                                                                                                                                                  
syntax match tsComment /\/\/.*/ contains=@Spell                                                                                                                                                                                                                                                                                                                                                                                                            
syntax match tsComment /\/\*\_.\{-}\*\// contains=@Spell                                                                                                                                                                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for regular expressions                                                                                                                                                                                                                                                                                                                                                                                                       
syntax match tsRegexp /\/\_.\{-}\// contained                                                                                                                                                                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for variables and function names                                                                                                                                                                                                                                                                                                                                                                                              
syntax match tsVariable /\v\w+/ contained                                                                                                                                                                                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for object properties                                                                                                                                                                                                                                                                                                                                                                                                         
syntax match tsObjectProperty /\v\.\w+/ contained                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for brackets and punctuation                                                                                                                                                                                                                                                                                                                                                                                                  
syntax match tsBrackets /[\[\]{}()]/ contained                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
" Define syntax highlighting for the '->' operator in TypeScript                                                                                                                                                                                                                                                                                                                                                                                           
syntax match tsArrowOperator /->/                                                                                                                                                                                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                                                                                                                                                                                                                           
